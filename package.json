{
  "name": "discord-crosspost-auto-translate",
  "version": "1.0.1",
  "description": "Automatically translates and replies to cross-posted messages.",
  "main": "dist/main.js",
  "repository": "git@github.com:book000/discord-crosspost-auto-translate.git",
  "author": "Tomachi",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "ts-node ./src/main.ts",
    "dev": "ts-node-dev ./src/main.ts",
    "start": "node ./dist/main.js",
    "compile": "tsc -p .",
    "compile:test": "tsc -p . --noEmit",
    "lint": "run-p -c lint:prettier lint:eslint lint:tsc",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:tsc": "tsc",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src"
  },
  "devDependencies": {
    "@types/config": "3.3.0",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "axios": "1.3.2",
    "config": "3.3.9",
    "discord.js": "13.12.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "prettier": "2.8.4",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.7.4",
    "webpack-node-externals": "3.0.0",
    "yarn-run-all": "3.1.1",
    "form-data": "4.0.0"
  }
}
